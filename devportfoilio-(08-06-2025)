    <!-- Main JS File -->
    <script src="assets/js/main.js"></script>

    <!-- Custom JS for Portfolio Carousels, Hero Background, and Typed Sync -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Hero Section: Typed.js and Background Swiper Sync
            const typedItems = [
                "R&D Engineer",
                "Team Lead NCRA-NEDUET",
                "Founder & CTO – RobAutoStem & Haptronica",
                "Industrial Automation Architect",
                "Embedded Systems Innovator",
                "IIoT & Haptics Expert",
                "Robotics Consultant",
                "International Teleoperator"
            ];
            const typingSpeed = 100; // ms per character
            const backSpeed = 50; // ms per character for backspacing
            const backDelay = 1000; // ms pause before backspacing
            const loopDelay = 1000; // ms pause after typing each string

            // Calculate total duration for one full typed.js cycle
            let totalCycleDuration = 0;
            typedItems.forEach(item => {
                // Time to type string
                const typeTime = item.length * typingSpeed;
                // Time to backspace string
                const backspaceTime = item.length * backSpeed;
                // Total time for this item (type + pause + backspace + pause)
                totalCycleDuration += typeTime + backDelay + backspaceTime + loopDelay;
            });

            // Initialize Typed.js
            let typedInstance;
            const heroSwiper = new Swiper('.hero-swiper', {
                loop: true,
                speed: 600,
                autoplay: {
                    delay: totalCycleDuration,
                    disableOnInteraction: false
                },
                effect: 'fade',
                allowTouchMove: false // Prevent manual swiping
            });

            typedInstance = new Typed('.typed', {
                strings: typedItems,
                typeSpeed: typingSpeed,
                backSpeed: backSpeed,
                backDelay: backDelay,
                loop: true,
                loopCount: Infinity,
                onComplete: function () {
                    // Ensure background swiper stays in sync
                    heroSwiper.autoplay.start();
                }
            });

            // Portfolio Carousels: Dynamic Video Duration
            function initializePortfolioSwiper(selector) {
                const defaultImageDelay = 5000; // 5 seconds for images
                let swiperInstance = new Swiper(selector, {
                    loop: true,
                    speed: 600,
                    slidesPerView: 1,
                    pagination: {
                        el: '.swiper-pagination',
                        clickable: true
                    },
                    navigation: {
                        nextEl: '.swiper-button-next',
                        prevEl: '.swiper-button-prev'
                    },
                    autoplay: {
                        disableOnInteraction: false
                    },
                    on: {
                        slideChange: function () {
                            // Pause all videos
                            const videos = document.querySelectorAll(selector + ' video');
                            videos.forEach(video => {
                                video.pause();
                                video.currentTime = 0; // Reset to start
                            });

                            // Determine delay based on slide type
                            const activeSlide = this.slides[this.activeIndex];
                            const slideType = activeSlide.getAttribute('data-type');
                            let delay;

                            if (slideType === 'video') {
                                const activeVideo = activeSlide.querySelector('video');
                                activeVideo.play().catch(error => console.log('Video play error:', error));
                                // Use video duration or fallback to 10 seconds if duration is not available
                                delay = (activeVideo.duration || 10) * 1000;
                            } else {
                                delay = defaultImageDelay;
                            }

                            // Update autoplay delay
                            this.autoplay.stop();
                            this.params.autoplay.delay = delay;
                            this.autoplay.start();
                        }
                    }
                });

                // Preload video metadata to ensure duration is available
                const videos = document.querySelectorAll(selector + ' video');
                videos.forEach(video => {
                    video.load(); // Trigger loading to fetch metadata
                });

                return swiperInstance;
            }

            // Initialize Swiper for each portfolio item
            initializePortfolioSwiper('.portfolio-swiper-ned-smt');
            initializePortfolioSwiper('.portfolio-swiper-pull-tester');
            initializePortfolioSwiper('.portfolio-swiper-mdof-arm');
            initializePortfolioSwiper('.portfolio-swiper-oxygen-analyzer');
            initializePortfolioSwiper('.portfolio-swiper-e-iot');
            initializePortfolioSwiper('.portfolio-swiper-ned-sill');
            initializePortfolioSwiper('.portfolio-swiper-cpap');
            initializePortfolioSwiper('.portfolio-swiper-metervision');
            initializePortfolioSwiper('.portfolio-swiper-shake-table');
            initializePortfolioSwiper('.portfolio-swiper-haptic-teleop');
            initializePortfolioSwiper('.portfolio-swiper-haptic-ve');
            initializePortfolioSwiper('.portfolio-swiper-electric-car');
        });
    </script>

</body>

