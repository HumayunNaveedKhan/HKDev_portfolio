                    <div class="row gy-4 isotope-container" data-aos="fade-up" data-aos-delay="200">
                        <!-- Client Interaction 1 -->
                        <div class="col-lg-4 col-md-6 network-item isotope-item filter-clients">
                            <picture>
                                <source srcset="assets/img/network/client-meeting-1.webp" type="image/webp">
