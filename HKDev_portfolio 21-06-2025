    <!-- Main JS File -->
    <script src="assets/js/main.js"></script>
    <!-- Custom JS -->
    <script>
        document.addEventListener('DOMContentLoaded', function () {
            // Initialize Typed.js for Hero Section
            new Typed('.typed', {
                strings: [
                    "R&D Engineer",
                    "Team Lead NCRA-NEDUET",
                    "Founder & CTO – RobAutoStem & Haptronica",
                    "Industrial Automation Architect",
                    "Embedded Systems Innovator",
                    "IIoT & Haptics Expert",
                    "Robotics Consultant",
                    "International Teleoperator"
                ],
                typeSpeed: 100,
                backSpeed: 50,
                backDelay: 2000,
                startDelay: 500,
                smartBackspace: true,
                loop: true,
                loopCount: Infinity
            });

            // Portfolio and Network Carousels
            function initializeSwiper(selector, isPortfolio = true) {
                const container = isPortfolio ? '.isotope-container' : '.network .isotope-container';
                const slides = document.querySelectorAll(selector + '.swiper-slide');
                const hasMultipleSlides = slides.length > 1;
                let swiperInstance = new Swiper(selector, {
                    loop: hasMultipleSlides,
                    speed: 600,
                    slidesPerView: 1,
                    pagination: {
                        el: '.swiper-pagination',
                        clickable: true,
                        renderBullet: function (index, className) {
                            return hasMultipleSlides ? '<span class="' + className + '"></span>' : '';
                        }
                    },
                    navigation: {
                        nextEl: '.swiper-button-next',
                        prevEl: '.swiper-button-prev',
                        disabledClass: 'swiper-button-disabled',
                        hiddenClass: 'swiper-button-hidden'
                    },
                    autoplay: {
                        delay: 5000,
                        disableOnInteraction: false
                    },
                    on: {
                        init: function () {
                            if (!hasMultipleSlides) {
                                this.el.querySelector('.swiper-button-next').style.display = 'none';
                                this.el.querySelector('.swiper-button-prev').style.display = 'none';
                                this.el.querySelector('.swiper-pagination').style.display = 'none';
                            }
                        },
                        slideChange: function () {
                            const videos = document.querySelectorAll(selector + ' video');
                            videos.forEach(video => {
                                video.pause();
                                video.currentTime = 0;
                            });
                            const activeSlide = this.slides[this.activeIndex];
                            const slideType = activeSlide.getAttribute('data-type');
                            if (slideType === 'video') {
                                const activeVideo = activeSlide.querySelector('video');
                                activeVideo.play().catch(error => console.log('Video play error:', error));
                                const delay = (activeVideo.duration || 10) * 1000;
                                this.autoplay.stop();
                                this.params.autoplay.delay = delay;
                                this.autoplay.start();
                            }
                        }
                    }
                });
                const videos = document.querySelectorAll(selector + ' video');
                videos.forEach(video => video.load());
                return swiperInstance;
            }

            // Initialize Portfolio Swipers
            const portfolioSwipers = [
                '.portfolio-swiper-ned-smt',
                '.portfolio-swiper-haptic-teleop',
                '.portfolio-swiper-haptic-ve',
                '.portfolio-swiper-ned-sill',
                '.portfolio-swiper-pull-tester',
                '.portfolio-swiper-mdof-arm',
                '.portfolio-swiper-oxygen-analyzer',
                '.portfolio-swiper-e-iot',
                '.portfolio-swiper-cpap',
                '.portfolio-swiper-metervision',
                '.portfolio-swiper-shake-table',
                '.portfolio-swiper-electric-car'
            ].map(selector => initializeSwiper(selector));

            // Initialize Isotope for Portfolio
            imagesLoaded('.isotope-container', function () {
                const iso = new Isotope('.isotope-container', {
                    itemSelector: '.portfolio-item',
                    layoutMode: 'masonry'
                });
                iso.layout();
            });

            // Initialize Isotope for Network
            imagesLoaded('.network .isotope-container', function () {
                const iso = new Isotope('.network .isotope-container', {
                    itemSelector: '.network-item',
                    layoutMode: 'masonry'
                });
                iso.layout();
            });

            // Preloader Fix
            window.addEventListener('load', function () {
                const preloader = document.getElementById('preloader');
                if (preloader) {
                    setTimeout(() => {
                        preloader.style.opacity = '0';
                        setTimeout(() => preloader.style.display = 'none', 500);
                    }, 500);
                }
            });
        });
    </script>
</body>
</html>
